#include "xparameters.h"
#include "sinewave_gen_peripheral.h"

//====================================================

#define MAX_SINEWAVE_DIV  5


XPAR_SINEWAVE_GEN_IP_SW_AXI_BASEADDR

void SinewaveGen_SetFrequencyDivider(uint8_t div) {
	uint8_t validated_div = div <
	SINEWAVE_GEN_PERIPHERAL_mWriteReg(XPAR_SINEWAVE_GEN_IP_SW_AXI_BASEADDR, 0x00, );

}


//====================================================


int main (void)
{


   XGpio dip, push;
   int i, psb_check, dip_check;

   xil_printf("-- Start of the Program --\r\n");

   XGpio_Initialize(&dip, XPAR_DIP_DEVICE_ID); // Modify this
   XGpio_SetDataDirection(&dip, 1, 0xffffffff);

   XGpio_Initialize(&push, XPAR_PUSH_DEVICE_ID); // Modify this
   XGpio_SetDataDirection(&push, 1, 0xffffffff);


   while (1)
   {
	  psb_check = XGpio_DiscreteRead(&push, 1);
	  xil_printf("Push Buttons Status %x\r\n", psb_check);
	  dip_check = XGpio_DiscreteRead(&dip, 1);
	  xil_printf("DIP Switch Status %x\r\n", dip_check);

	  // output dip switches value on LED_ip device

	  for (i=0; i<9999999; i++);
   }
}
